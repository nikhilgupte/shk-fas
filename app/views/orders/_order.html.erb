<tr id="o_<%= order.id %>" class="<%= cycle 'odd', 'even' %>">
<td><%= h(order.product.code) %></td><td><%= h order.product.name %></td>
<td class="numeric"><%= number_with_precision(order.quantity, 3) %></td>
<td class="numeric"><%= number_with_precision(order.production_quantity, 3) %></td>
<td><%= order.location_text %></td>
<td><%= order.priority_text %></td>
<td class="tip numeric"><%= time_ago_in_words order.created_at %> ago</td>
<td><%= link_to_remote 'delete', :confirm => 'Delete this order?', :url => {:action => :destroy, :id => order.id}, :method => :delete, :loading => 'Element.remove("o_'+order.id.to_s+'")' %></td></tr>
